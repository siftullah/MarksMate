                </header>
                <div class = "expbox-table">
                <table role="table" class variant="simple" color="gray-500" mb="24px">
                    <thead>
                    <tr role="row">
                        <th colspan="1" role="columnheader" title="Toggle SortBy" class="col1" style="padding-left:1rem;cursor: pointer;">
                            <div class="head1">ID
                            </div>
                        </th>
                        <th colspan="1" role="columnheader" title="Toggle SortBy" class="col1" style="padding-left:1rem;cursor: pointer;">
                        <div class="head1">RollNo
                        </div>
                        </th>
                        <th colspan="1" role="columnheader" title="Toggle SortBy" class="col1" style="cursor: pointer;">
                        <div class="head1">Text
                        </div>
                        </th>
                        <th colspan="1" role="columnheader" title="Toggle SortBy" class="col1" style="cursor: pointer;">
                        <div class="head1">Title
                        </div>
                        </th>
                        <th colspan="1" role="columnheader" title="Toggle SortBy" class="col1" style="cursor: pointer;">
                            <div class="head1">Date
                            </div>
                        </th>
                    </tr>
                    </thead>
                    <tbody role="rowgroup">
                        <% data7.forEach((row,index) =>{ %>
                        <tr role="row">
                        <td role="cell" class="row1">
                            <div class="row1-1">
                                <p style class="row-font">
                                    <%= typeof data9[index] !== 'undefined' && typeof data9[index].id !== 'undefined' ? data9[index].id : '-' %>
                                </p>
                            </div>
                            </td>
                        <td role="cell" class="row1">
                        <div class="row1-1">
                            <a href="https://mail.google.com/mail/?view=cm&fs=1&to=<%= encodeURIComponent(data8[index].email) %>&su=<%= encodeURIComponent(row.notification_title) %>&body=<%= encodeURIComponent(row.notification_text) %>&authuser=2" target="_blank" 
                                aria-label="Email <%= typeof data8[index] !== 'undefined' && typeof data8[index].email !== 'undefined' ? data8[index].email : '-' %>" 
                                data-tooltip-enabled="true" 
                                data-tooltip-id="tt-c10">
                                <p class="row-font">
                                    <%= typeof data9[index] !== 'undefined' && typeof data9[index].rollNo !== 'undefined' ? data9[index].rollNo : '-' %>
                            </a>
                        </div>
                        </td>
                        <td role="cell" class="row1">
                        <p class="row-font">
                            <%= typeof row !== 'undefined' && typeof row.notification_text !== 'undefined' ? row.notification_text : '-' %>
                        </p>
                        </td>
                        <td role="cell" class="row1">
                        <p class="row-font">
                            <%= typeof row !== 'undefined' && typeof row.notification_title !== 'undefined' ? row.notification_title : '-' %>
                        </p>
                        </td>
                        <td role="cell" class="row1">
                            <p class="row-font"><%
                                let date = new Date(row.notification_date);
                                let year = date.getFullYear();
                                let month = date.getMonth() + 1;
                                let day = date.getDate();
                                let formattedDate = `${year}-${month < 10 ? '0' + month : month}-${day < 10 ? '0' + day : day}`;
                                %>
                                <%= typeof formattedDate !== 'undefined' ? formattedDate : '-' %>
                            </p>
                        </td>
                    </tr>
                    <% })%>

                    </tbody>
                </table>
            </div>
        <script>    
        google.charts.load('current', {'packages':['bar','corechart']});
        google.charts.setOnLoadCallback(drawChart);

        var data5 = "<%= data5[0]['']%>";
        var data6 = "<%= data6[0]['']%>";

        function drawChart() {

        var data1 = google.visualization.arrayToDataTable([
            ['Gender', 'Total Students'],
            ['Male',     parseInt(data5)],
            ['Female',    parseInt(data6)]
        ]);

        var chart1 = new google.visualization.PieChart(document.getElementById('piechart'));
        
        chart1.draw(data1);

        }
        </script>
