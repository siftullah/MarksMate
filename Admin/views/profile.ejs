<div class = screen2>
    
</div>

<div class = "profile">
    <div class = "profile-box">
        <div class = "general">
            <div class = "general-h">
                <h4 class = "general-title">General Information</h4>
            </div>
        </div>
        <div class = "general-box">
            <div class = "general-box1">
                <p class = "general-head">UserId</p>
                <p class = "general-head1"><%= data1[0].userId %></p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">User Type</p>
                <p class = "general-head1">Admin</p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">Campus</p>
                <p class = "general-head1"><%= data[0].campus_name %></p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">Status</p>
                <p class = "general-head1">Current</p>
            </div>                            
        </div>
    </div>
</div>


<div class = "profile">
    <div class = "profile-box">
        <div class = "general">
            <div class = "general-h">
                <h4 class = "general-title">Personal Information</h4>
            </div>
        </div>
        <div class = "general-box">
            <div class = "general-box1">
                <p class = "general-head">Full Name</p>
                <p class = "general-head1"><%= data[0].adminName %></p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">Gender</p>
                <p class = "general-head1"><%= data[0].gender %></p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">DOB</p>
                <p class = "general-head1">
                <%
                let date = new Date(data[0].admin_dob);
                let year = date.getFullYear();
                let month = date.getMonth() + 1;
                let day = date.getDate();
                let formattedDate = `${year}-${month < 10 ? '0' + month : month}-${day < 10 ? '0' + day : day}`;
                %>
                <%= formattedDate %></p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">Blood Group</p>
                <p class = "general-head1"><%= data[0].bloodGroup %></p>
            </div>


            <div class = "general-box1">
                <p class = "general-head">CNIC</p>
                <p class = "general-head1"><%= data[0].cnic %></p>
            </div>


            <div class = "general-box1">
                <p class = "general-head">Email</p>
                <p class = "general-head1"><%= data[0].email %></p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">Address</p>
                <p class = "general-head1"><%= data[0].adminAddress %></p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">Mobile No</p>
                <p class = "general-head1"><%= data[0].mobileNo %></p>
            </div>

            <div class = "general-box1">
                <p class = "general-head">Campus</p>
                <p class = "general-head1"><%= data[0].campus_name %></p>
            </div>

            
        </div>
    </div>
</div>



